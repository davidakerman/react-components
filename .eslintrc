{
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors",
      "eslint:recommended",
      "plugin:react/recommended"
   ],
    "rules": {
      "no-use-before-define": 0,
      "import/prefer-default-export": 1,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "react/display-name": 1,
      "react/prop-types":1,
      "no-console": 0,
      "import/default": 1,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "react/jsx-key": 0,
      "semi": 2,
      "prettier/prettier": ["error", {
        "printWidth": 120,
        "singleQuote": true,
        "parser": "babylon"
      }]
    },
    "plugins": ["react", "json", "prettier"]
  }
  